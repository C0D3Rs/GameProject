@model SetItemImageViewModel

@{
    ViewBag.Title = "Ustaw Obrazek";
}

<h2>SetItemImage</h2>

<table class="table">
    <tr>
        <th>#</th>
        <th>Nazwa</th>
        <th></th>
    </tr>

@foreach (var item in Model.Images) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FileName)
        </td>
        <td>
            @Html.ActionLink("Ustaw", Model.ActionReturn, Model.ControllerReturn, new { ImageId = item.ID }, null)
        </td>
    </tr>
}
</table>
